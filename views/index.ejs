<%- include("layouts/header.ejs")%>

    <body>
        <%- include("layouts/navbar.ejs")%>
            <div class="container">
                </br>
                <h3>Perguntas</h3>
                <hr>
                <a href="/perguntar" class="btn btn-primary">Perguntar</a>
                </br>
                <% quests.forEach(quests=> { %>
                </br>
                    <div class="card">
                        <div class="card-body">
                            <h3>
                                <p><%= quests.titulo %></p>
                            </h3>
                            <h3>
                                <p><%= quests.descricao %></p>
                            </h3>
                        </div>
                        <div class="card-footer">
                            <a href="/pergunta/<%= quests.id %>" class="btn btn-primary">Resposta</a>
                        </div>
                    </div>

                    <% }) %>

            </div>
    </body>

    <%- include("layouts/footer.ejs")%>

